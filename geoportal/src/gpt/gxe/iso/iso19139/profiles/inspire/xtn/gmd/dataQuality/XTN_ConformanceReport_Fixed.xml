<?xml version="1.0" encoding="UTF-8"?>
 <!-- simplified conformance report 
     (gmd:report/gmd:DQ_DomainConsistency/gmd:result/gmd:DQ_ConformanceResult -->
<g:element xmlns:g="http://www.esri.com/geoportal/gxe" 
           xmlns:h="http://www.esri.com/geoportal/gxe/html"
           xmlns:gco="http://www.isotc211.org/2005/gco"
           xmlns:gmd="http://www.isotc211.org/2005/gmd"
           g:targetName="gmd:report" g:minOccurs="1" g:maxOccurs="unbounded"
           g:label="$i18n.catalog.iso19139.DQ_DataQuality.report"
           g:extends="$base/xtn/ui/UI_Element.xml">
  <g:body>
    <!-- fixed element for commission regulation -->
    <g:element g:targetName="gmd:DQ_DomainConsistency" g:minOccurs="0" g:maxOccurs="1"  h:tag="div" g:jsClass="gxe.control.Element">
      <g:element g:targetName="gmd:result" g:minOccurs="1" g:maxOccurs="1" g:label="$i18n.catalog.iso19139.DQ_ConformanceResult" g:i18nBase="catalog.iso19139.DQ_ConformanceResult" g:extends="$base/xtn/ui/UI_Element.xml">
        <g:body>
          <g:element g:targetName="gmd:DQ_ConformanceResult" g:minOccurs="1" g:maxOccurs="1"  g:extends="$base/xtn/ui/UI_Element.xml">
            <g:body>		        
              <g:element g:targetName="gmd:specification" g:minOccurs="1" g:maxOccurs="1"  g:extends="$base/xtn/ui/UI_Element.xml">
               <g:body>
                <g:element g:targetName="gmd:CI_Citation" g:minOccurs="1" g:maxOccurs="1" g:i18nBase="catalog.iso19139.CI_Citation"  g:extends="$base/xtn/ui/UI_Element.xml">
                  <g:body>
                    <g:element g:targetName="gmd:title" g:minOccurs="1" g:maxOccurs="1" g:label="$i18n.catalog.iso19139.CI_Citation.specification.title" g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml">
                      <g:body>
                        <g:element g:value="COMMISSION REGULATION (EU) No 1089/2010 of 23 November 2010 implementing Directive 2007/2/EC of the European Parliament and of the Council as regards interoperability of spatial data sets and services">
                        </g:element>
                      </g:body>
                    </g:element>

                    <g:element g:targetName="gmd:date" g:minOccurs="1" g:maxOccurs="1" g:label="$i18n.catalog.iso19139.CI_Citation.specification.date" g:extends="$base/xtn/ui/UI_Element.xml">
                      <g:body>
                        <g:element g:targetName="gmd:CI_Date" g:minOccurs="1" g:maxOccurs="1" g:label="CI_Date" g:extends="$base/xtn/ui/UI_Element.xml">
                          <g:body>
                            <g:element g:targetName="gmd:date" g:minOccurs="1" g:maxOccurs="1" g:label="date" g:extends="$base/schema/gco/basicTypes/Date_PropertyType.xml">
                              <g:body>
                                <g:element g:value="2010-12-08"></g:element>
                              </g:body>
                            </g:element>
                            <g:element g:targetName="gmd:dateType" g:minOccurs="1" g:maxOccurs="1" g:label="dateType" g:extends="$base/xtn/ui/UI_Element.xml">
                              <g:body>
                                <g:element g:targetName="gmd:CI_DateTypeCode" g:minOccurs="1" g:maxOccurs="1" g:label="CI_DateTypeCode" g:extends="$base/xtn/ui/UI_Element.xml">
                                  <g:body>
                                    <g:attribute g:targetNS="" g:targetName="codeList" g:use="required" g:value="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#CI_DateTypeCode" g:extends="$base/core/xml/Attribute.xml"></g:attribute>
                                    <g:attribute g:targetNS="" g:targetName="codeListValue" g:use="required" g:value="publication" g:extends="$base/core/xml/Attribute.xml"></g:attribute>
                                    <g:textNode g:value="publicationtextnode" g:extends="$base/core/xml/TextNode.xml">
                                      <g:body>
                                        <g:input g:overridable="true" g:value="xyz" g:extends="$base/core/ui/InputText.xml"/>
                                      </g:body> 
                                    </g:textNode>
                                  </g:body>
                                </g:element>
                              </g:body>
                            </g:element>
                          </g:body>
                        </g:element>
                      </g:body>
                    </g:element>
                  </g:body>                   
              </g:element>
             </g:body>                  
            </g:element>
              
            <!-- an explanation of the result -->
            <g:element g:targetName="gmd:explanation" g:minOccurs="1" g:maxOccurs="1" g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml">
              <g:body>
                <g:element g:value="None"></g:element>
              </g:body>
            </g:element> 

            <!-- degree of conformance (pass/fail) -->
            <g:element g:targetName="gmd:pass" g:minOccurs="1" g:maxOccurs="1" g:serializeIfEmpty="true" g:extends="$base/schema/gco/basicTypes/Boolean_PropertyType.xml">
              <g:body>
                <g:element g:value="true"></g:element>
              </g:body>
            </g:element>
          </g:body>
          </g:element>
        </g:body>
      </g:element>		  
    </g:element>        
  </g:body>
</g:element>